@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<input type="number" id="txtNumber" />
<input type="submit" id="btnGetByID" value="AdminGetByID" />
<input type="submit" id="btnAdminApi" value="AdminApi" />
<input type="submit" id="btnAdminApi1" value="AdminApi1" />
<input type="submit" id="btnEmployeeApi" value="EmployeeApi" />
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {

        //for authorization headers
        var headers = {};

        headers.Authorization = 'Bearer ' + localStorage.getItem('access_token');

        $('#btnGetByID').click(function () {
            $.ajax({
                type: 'GET',
                headers: headers,
                dataType: 'JSON',
                url: 'api/Admin/GetByAllById/{Id}',
                data:{Id :$('#txtNumber').val() },
                success: function (data) {
                    alert(data);
                },
                error: function (error) {
                    alert(error.responseText);
                }
            });
        });

        $('#btnAdminApi').click(function () {
            $.ajax({
                type: 'GET',
                headers: headers,
                dataType: 'JSON',
                url: 'api/Admin/Get',
                success: function (data)
                {
                    alert(data);
                },
                error: function (error)
                {
                    alert(error.responseText);
                }
            });
        });

        $('#btnAdminApi1').click(function () {
            $.ajax({
                type: 'GET',
                headers: headers,
                dataType: 'JSON',
                url: 'api/Admin/GetByAll',
                success: function (data) {
                    alert(data);
                },
                error: function (error) {
                    alert(error.responseText);
                }
            });
        });

        $('#btnEmployeeApi').click(function () {
            $.ajax({
                type: 'GET',
                headers: headers,
                dataType: 'JSON',
                url: 'api/Employee/Get',
                success: function (data) {
                    alert(data);
                },
                error: function (error) {
                    alert(error.responseText);
                }
            });
        });
    });
</script>