	</section>
	<hr>
	<!--******************** Services Section ********************-->
	<section id="services" class="single-page scrollblock wow lightSpeedIn" data-wow-duration="10s">
		<div class="container">
			<div class="align"><i class="icon-cog-circled"></i></div>
			<h1>@Resource.Services</h1>
			<div class="row">
			   @foreach (var item in Model.foodItems_Details)
			   {
				<div class="span3">
					<div class="align"> 
						<img src="@Url.Content(item.ImagePath)" width="80" height="80" class="team-thumb img-circle"/>
					 </div>
					<h2>@item.Description</h2>
					<p>Price:@item.NewPrice-Kyats</p>
					<p>Category:@item.Category.Description</p>
					<p>Today Special:@item.IsTodaySpecial</p>
				</div>
			   }          
			</div>
		</div>
	</section>
	@*<hr>*@
	<!--******************** Testimonials Section ********************-->
	<section id="testimonials" class="single-page hidden-phone wow flipInY" data-wow-duration="4s">
		<div class="container">
			<div class="row">
				<div class="blockquote-wrapper">
					<blockquote class="mega">
						<div class="span4">
							<p class="cite">John Doe &amp; Sons:</p>
						</div>
						<div class="span8">
							<p class="alignright">"We highly appreciate the enthusiasm and creative talent of the people at Legend!"</p>
						</div>
					</blockquote>
				</div>
				<!-- /.blockquote-wrapper -->
			</div>

		</div>

	</section>
	<hr>
	<!--******************** News Section ********************-->
	<section id="news" class="single-page scrollblock wow flipInX" data-wow-duration="20s">
		<div class="container">
			<div class="align"><i class="icon-pencil-circled"></i></div>
			<h1>@Resource.News</h1>
			<div class="row">
				<article class="span4 post">
					<img class="img-news" src="~/Scripts/ContactUs/img/blog_img-01.jpg" alt="">
					<div class="inside">
						<p class="post-date"><i class="icon-calendar"></i> March 17, 2013</p>
						<h2>A girl running on a road</h2>
						<div class="entry-content">
							<p>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s. &hellip;</p>
							<a href="#" class="more-link">read more</a>
						</div>
					</div>

				</article>

				<article class="span4 post">
					<img class="img-news" src="~/Scripts/ContactUs/img/blog_img-02.jpg" alt="">
					<div class="inside">
						<p class="post-date">February 28, 2013</p>
						<h2>A bear sleeping on a tree</h2>
						<div class="entry-content">
							<p>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s. &hellip;</p>
