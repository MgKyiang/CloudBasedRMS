	<script type="text/javascript">
	//==============================================>
	// Page Load Event
	//==============================================>
	$(document).ready()
	{
		//==========
		//alert view
		//==========
		window.setTimeout(function () {
			$(".alert").fadeTo(500, 0).slideUp(500, function () {
				$(this).remove();
			});
		}, 4000);
	}
	</script>
</body>
